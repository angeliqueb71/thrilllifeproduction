    <div class="container">
      <div class="row">
        <div class="col col-lg-5">
          <p>Copyright &copy;
              <span id="year"></span>
              <a href="/"><%= image_tag("record3ps.png", :alt=> "main", :size=>"20x20") %></a>
              <span class="White">  Thrill Life Productions</span></p>
        </div>
        <div class="col-lg-7">
          <ul class="list-inline social-links">
              <li class="list-inline-item social-link">
                  <a href="https://www.facebook.com/ThrillLifeProductions"><i class="fab fa-facebook" style="font-size:25px;color:white"></a></i></li>

              <li class="list-inline-item social-link">
                  <a href="https://twitter.com/THRILLEST_1"><i class="fab fa-twitter" style="font-size:25px;color:white"></a></i></li>

              <li class="list-inline-item social-link">
                <a href="https://www.instagram.com/thrill_life_productions/"><i class="fab fa-instagram" style="font-size:25px;color:white"></a></i></li>
          </ul>
        </div>
      </div>
    </div>
  </footer>

    <!--footer end -->

    <script src="http://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
